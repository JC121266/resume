<view class="editResumeStyle" bind:touchstart="handleTouchstart" bind:touchmove="handleTouchmove" bind:touchend="handleTouchend">
    <!-- <view>editResume: left right slid and paging</view> -->
    <view class="infoCardStyle baseInfoCard {{card === 'baseInfoCard'?'showCard':''}}">
        <form bindsubmit="handleFormSubmit">
            <view>
                <view>userName:</view>
                <input name="userName" value="{{ baseInfo.userName }}" placeholder="please inpiut your name" />
            </view>
            <view>
                <view>userGender:</view>
                <input name="userGender" value="{{ baseInfo.userGender }}"  placeholder="please inpiut your userGender" />
            </view>
            <view>
                <view>birthData:</view>
                <input name="birthData" value="{{ baseInfo.birthData }}"  placeholder="please inpiut your birthData" />
            </view>
            <view>
                <view>eMail:</view>
                <input name="eMail" value="{{ baseInfo.eMail }}"  placeholder="please inpiut your eMail" />
            </view>
            <button form-type="submit">submit</button>
        </form>
    </view>
