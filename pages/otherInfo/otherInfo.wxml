<view class="otherInfoStyle showInformationCardStyle">
    <block wx:for="{{ otherInfo }}" wx:key="{{ index }}">
        <view wx:if="{{ !nullData }}">
            <text>{{ item['title' + index] }}</text>
            <view>{{ item['content' + index] }}</view>
        </view>
        <view wx:if="{{ nullData }}">nullData</view>
    </block>
</view>